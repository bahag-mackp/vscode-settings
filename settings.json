{
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[latex]": {
        "editor.defaultFormatter": "nickfode.latex-formatter"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "breadcrumbs.enabled": false,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.userWords": [
        "apidevtools",
        "APIGEE",
        "apiproduct",
        "bahag",
        "commandlineinprogress",
        "dbaeumer",
        "defaultsauto",
        "Dejavu",
        "dmodel",
        "dont",
        "easymotioninputmode",
        "easymotionmode",
        "esbenp",
        "lkup",
        "Loca",
        "Masterdata",
        "metatype",
        "nestjs",
        "nickfode",
        "Packstation",
        "Pluscard",
        "searchinprogressmode",
        "surroundinputmode",
        "visualblock",
        "visualline",
        "vsicons",
        "yzhang"
    ],
    "chat.commandCenter.enabled": false,
    "debug.console.fontFamily": "LiterationMono Nerd Font",
    "debug.console.fontSize": 13,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "LiterationMono Nerd Font",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.glyphMargin": true,
    "editor.inlayHints.enabled": "off",
    "editor.inlineSuggest.enabled": true,
    "editor.lineNumbers": "on",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.minimap.size": "fill",
    "editor.mouseWheelZoom": true,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "eslint.format.enable": true,
    "files.autoSave": "onFocusChange",
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "markdown.preview.fontSize": 14,
    "output.smartScroll.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontFamily": "LiterationMono Nerd Font",
    "terminal.integrated.fontSize": 12,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.commandlineinprogress": "#3B4252",
    "vim.statusBarColors.easymotioninputmode": "#46525C",
    "vim.statusBarColors.easymotionmode": "#3B4252",
    "vim.statusBarColors.insert": "#7A4246",
    "vim.statusBarColors.normal": [
        "#282c3b",
        "#ffffff"
    ],
    "vim.statusBarColors.replace": "#78553A",
    "vim.statusBarColors.searchinprogressmode": "#3B4252",
    "vim.statusBarColors.surroundinputmode": "#506070",
    "vim.statusBarColors.visual": "#495A47",
    "vim.statusBarColors.visualblock": "#3D4A57",
    "vim.statusBarColors.visualline": "#5E5638",
    "vsicons.dontShowNewVersionMessage": true,
    "window.density.editorTabHeight": "compact",
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.location": "bottom",
    "workbench.colorCustomizations": {
        "editor.wordHighlightBackground": "#ffffff00",
        "editor.wordHighlightBorder": "#ffffff74",
        "editorCursor.foreground": "#ffffff",
        "statusBar.background": "#5E5638",
        "statusBar.debuggingBackground": "#5E5638",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.foreground": "#ffffff",
        "statusBar.noFolderBackground": "#5E5638"
    },
    "workbench.colorTheme": "Dracula Theme",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.layoutControl.enabled": false,
    "workbench.preferredDarkColorTheme": "GitHub Dark",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.enableStickyScroll": false,
    "workbench.tree.indent": 18,
    "workbench.tree.renderIndentGuides": "always",
    "errorLens.fontFamily": "Lekton Nerd Font Mono",
    "errorLens.fontSize": "14",
    "errorLens.fontWeight": "100",
    "errorLens.messageTemplate": "[$source] $message",
    "errorLens.borderRadius": "0",
    "gitlens.currentLine.fontSize": 12,
    "gitlens.currentLine.fontFamily": "Lekton Nerd Font Mono",
    "githubPullRequests.pullBranch": "never",
    "search.showLineNumbers": true,
    "LineNumberDeco.activeForeground": "#f7f700",
    "LineNumberDeco.foregroundColorOfRepeatingDigits": "#141414",
    "window.commandCenter": false,
}